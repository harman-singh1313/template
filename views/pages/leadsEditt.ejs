                                            <th>#</th>
                                            <th>First name</th>
                                            <th>Last name</th>
                                            <th>Email</th>
                                            <th>Phone</th>
                                            <th>password</th>
                                            <th>Add on</th>

                                            <th></th>
                                        </tr>
                                    </thead>
                                    <tbody>

                                        <% result.forEach(function(x,srno){%>
                                            <tr>

                                                <td>
                                                    <%=++srno%>
                                                </td>
                                                <td>
                                                    <%=x.First_name%>
                                                </td>
                                                <td>
                                                    <%=x.Last_name%>
                                                </td>

                                                <td>
                                                    <%=x.email%>
                                                </td>
                                                <td>
                                                    <%=x.phone%>
                                                </td>
                                                <td>
                                                    <%=x.password%>
                                                </td>
                                                <td>
                                                    <%=x.addon%>
                                                </td>



                                                <td class="table-action">
                                                    <a href="/leadsEdit?id=<%= x._id %>"> <i
                                                            class="fa fa-pencil"></i></a>
                                                    <a href="#" class="delete-row"><i class="fa fa-trash-o"></i></a>
                                                </td>

                                            </tr>
                                            <% }); %>
